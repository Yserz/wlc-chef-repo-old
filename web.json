{
  "java": {
    "install_flavor": "oracle",
    "jdk_version": "7"
    "oracle": {
      "accept_oracle_download_terms": true
    }
  },
  "jenkins": {
    "server": {
      "install_method": "package",
      "port": 8080
    }
  },
  "mysql": {
    "server_root_password": "", 
    "server_debian_password": "", 
    "server_repl_password": "",
    "bind_address": "127.0.0.1",
    "port": 3306
  },
  "wlcserver": {
    "database": "coding",
    "test_database": "coding_test",
    "database_username": "wlcapp",
    "database_password": "",
	"glassfish": {
		"domain_name": "domain1",
		"admin_name": "admin",
		"admin_password": "",
		"admin_port": 4848,
		"secure": true,
		"echo": true,
		"terse": false,
		"jdbc_connection_pool": {
			"name": "CodingConnectionPool"
		},
		"jdbc_resource": {
			"jndi-name": "jdbc/coding"
		}
	}
  },
  "run_list": [ 
    "recipe[apt]", 
    "recipe[htop]",
    "recipe[wlcserver]"
  ]
}
