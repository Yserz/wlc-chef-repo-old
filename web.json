{
  "java": {
    "install_flavor": "oracle",
    "jdk_version": "7",
    "oracle": {
      "accept_oracle_download_terms": true
    }
  },
  "jenkins": {
    "server": {
      "install_method": "package",
      "port": 8080
    }
  },
  "mysql": {
    "server_root_password": "", 
    "server_debian_password": "", 
    "server_repl_password": "",
    "bind_address": "127.0.0.1",
    "port": 3306,
	"nice": 0,
	"service_name": "mysql",
	"basedir": "/usr",
	"data_dir": "/var/lib/mysql",
	"root_group": "root",
	"mysqladmin_bin": "/usr/bin/mysqladmin",
	"mysql_bin": "/usr/bin/mysql",
	"conf_dir": "/etc/mysql",
	"confd_dir": "/etc/mysql/conf.d",
	"socket": "/var/run/mysqld/mysqld.sock",
	"pid_file": "/var/run/mysqld/mysqld.pid",
	"old_passwords": 0,
	"grants_path": "/etc/mysql/grants.sql",
	"auto-increment-increment": 1,
	"auto-increment-offset": 1,
	"allow_remote_root": false,
	"remove_anonymous_users": false,
	"remove_test_database": false,
	"root_network_acl": null,
	"tmpdir": "/tmp",
	"log_files_in_group": false,
	"innodb_status_file": false,
	"tunable": {
		"character-set-server": "utf8",
		"collation-server": "utf8_general_ci",
		"lower_case_table_names": null,
		"back_log": "128",
		"key_buffer_size": "256M",
		"myisam_sort_buffer_size": "8M",
		"myisam_max_sort_file_size": "2147483648",
		"myisam_repair_threads": "1",
		"myisam-recover": "BACKUP",
		"max_allowed_packet": "16M",
		"max_connections": "800",
		"max_connect_errors": "10",
		"concurrent_insert": "2",
		"connect_timeout": "10",
		"tmp_table_size": "32M",
		"net_read_timeout": "30",
		"net_write_timeout": "30",
		"table_cache": "128",
		"thread_cache_size": 8,
		"thread_concurrency": 10,
		"thread_stack": "256K",
		"sort_buffer_size": "2M",
		"read_buffer_size": "128k",
		"read_rnd_buffer_size": "256k",
		"join_buffer_size": "128k",
		"wait_timeout": "180",
		"open-files-limit": "1024",
		"sql_mode": null,
		"skip-character-set-client-handshake": false,
		"skip-name-resolve": false,
		"slave_compressed_protocol": 0,
		"server_id": null,
		"log_bin": null,
		"log_bin_trust_function_creators": false,
		"relay_log": null,
		"relay_log_index": null,
		"log_slave_updates": false,
		"sync_binlog": 0,
		"skip_slave_start": false,
		"read_only": false,
		"log_error": null,
		"log_warnings": false,
		"log_queries_not_using_index": true,
		"log_bin_trust_function_creators": false,
		"innodb_log_file_size": "5M",
		"innodb_buffer_pool_size": "128M",
		"innodb_buffer_pool_instances": "4",
		"innodb_additional_mem_pool_size": "8M",
		"innodb_data_file_path": "ibdata1:10M:autoextend",
		"innodb_flush_method": false,
		"innodb_log_buffer_size": "8M",
		"innodb_write_io_threads": "4",
		"innodb_io_capacity": "200",
		"innodb_file_per_table": true,
		"innodb_lock_wait_timeout": "60",
		"innodb_thread_concurrency": "8",
		"innodb_commit_concurrency": "8",
		"innodb_read_io_threads": "8",
		"innodb_flush_log_at_trx_commit": "1",
		"innodb_support_xa": true,
		"innodb_table_locks": true,
		"skip-innodb-doublewrite": false,
		"transaction-isolation": null,
		"query_cache_limit": "1M",
		"query_cache_size": "16M",
		"log_slow_queries": "/var/log/mysql/slow.log",
		"slow_query_log": ,
		"long_query_time": 2,
		"expire_logs_days": 10,
		"max_binlog_size": "100M",
		"binlog_cache_size": "32K",
		"event_scheduler": 0,
		"table_open_cache": "128"
	}
  },
  "wlcserver": {
    "database": "coding",
    "test_database": "coding_test",
    "database_username": "wlcapp",
    "database_password": "",
	"glassfish": {
		"domain_name": "domain1",
		"admin_name": "admin",
		"admin_password": "",
		"admin_port": 4848,
		"secure": false,
		"echo": true,
		"terse": false,
